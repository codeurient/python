class Duzbucaqli:
    def __init__(self, length, width):
        self.length = length
        self.width = width

class Kvadrat(Duzbucaqli):
    def __init__(self, length, width):
        super().__init__(length, width)

    def area(self):
        return self.length * self.width
    
class Kub(Duzbucaqli):
    def __init__(self, length, width, height):
        self.length = length
        self.width = width
        self.height = height

square = Kvadrat(3, 3)
kub = Kub(3, 3, 3)